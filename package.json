{
    "name": "eslint-config",
    "version": "0.0.1",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{js,ts,tsx,md}\""
    },
    "peerDependencies": {
        "eslint": "^8.36.0"
    },
    "devDependencies": {
        "eslint": "^8.36.0",
        "prettier": "^2.8.7",
        "turbo": "^1.8.5"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "packageManager": "yarn@3.5.0"
}
